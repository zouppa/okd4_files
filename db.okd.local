api.lab.okd.local.    IN    A    192.168.1.240
api-int.lab.okd.local.    IN    A    192.168.1.240
*.apps.lab.okd.local.    IN    A    192.168.1.240
etcd-0.lab.okd.local.    IN    A     192.168.1.241
console-openshift-console.apps.lab.okd.local.     IN     A     192.168.1.240
oauth-openshift.apps.lab.okd.local.     IN     A     192.168.1.240

